$TTL 86400
@   IN  SOA ns.internal.lan. admin.internal.lan. (
        2023101001 ; Serial
        3600       ; Refresh
        1800       ; Retry
        604800     ; Expire
        86400      ; Negative Cache TTL
)

; Name servers
@   IN  NS  ns.internal.lan.

; A records for name servers
ns  IN  A   192.168.1.10

; A records for other hosts
web1    IN  A   192.168.1.20
web2    IN  A   192.168.1.21

; CNAME records
www     IN  CNAME   web1.internal.lan.